{
  "name": "sitecore-sxa-component-library",
  "version": "1.0.0",
  "description": "Comprehensive Sitecore SXA Component Library Simulator with Storybook",
  "main": "index.js",
  "scripts": {
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "sass": "sass src/styles:dist/css --watch",
    "sass:build": "sass src/styles:dist/css --style compressed",
    "dev": "concurrently \"npm run sass\" \"npm run storybook\"",
    "build": "npm run sass:build && npm run build-storybook"
  },
  "keywords": [
    "sitecore",
    "sxa",
    "component-library",
    "storybook",
    "bootstrap"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@storybook/addon-a11y": "^7.6.6",
    "@storybook/addon-essentials": "^7.6.6",
    "@storybook/addon-interactions": "^7.6.6",
    "@storybook/addon-links": "^7.6.6",
    "@storybook/blocks": "^7.6.6",
    "@storybook/html": "^7.6.6",
    "@storybook/html-webpack5": "^7.6.6",
    "@storybook/test": "^7.6.6",
    "concurrently": "^8.2.2",
    "css-loader": "^6.8.1",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.3",
    "storybook": "^7.6.6",
    "style-loader": "^3.3.3",
    "webpack": "^5.89.0"
  },
  "dependencies": {
    "bootstrap": "^5.3.2",
    "jquery": "^3.7.1"
  }
}
